{
  "name": "@iconsauce/core",
  "version": "1.2.2",
  "description": "Includes only the SVG icons matched in your code and generates a CSS icon font set.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && tsc --declaration && npm run lib",
    "clean": "rm -rf build || true && rm -rf lib || true",
    "dev": "tsc --watch",
    "lib": "cp -r ./build/src ./lib",
    "lint": "eslint --ignore-path .eslintignore",
    "prepublishOnly": "npm run test",
    "test": "npm run build && jest"
  },
  "bin": {
    "iconsauce": "lib/cli.js"
  },
  "files": [
    "lib",
    "src/interface"
  ],
  "dependencies": {
    "@iconsauce/config": "0.4.0",
    "@iconsauce/material-icons": "1.0.3",
    "@iconsauce/mdi-svg": "1.0.8",
    "@iconsauce/mgg-icons": "1.0.9",
    "@iconsauce/plugin": "0.0.1",
    "arg": "5.0.2",
    "chalk": "4.0.0",
    "css-loader": "6.8.1",
    "fast-glob": "3.3.1",
    "handlebars": "4.7.8",
    "lilconfig": "2.0.4",
    "recursive-copy": "2.0.14",
    "svg2ttf": "6.0.3",
    "svgicons2svgfont": "10.0.6"
  },
  "devDependencies": {
    "material-design-icons-updated": "7.0.0",
    "@material-icons/svg": "1.0.33",
    "@maggioli-design-system/svg-icons": "3.11.7",
    "@mdi/svg": "7.3.67",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-typescript": "7.23.2",
    "@types/debug": "4.1.10",
    "@types/jest": "29.5.6",
    "@types/node": "^16.11.26",
    "@types/react": "18.2.31",
    "@types/svg2ttf": "5.0.2",
    "@types/svgicons2svgfont": "10.0.4",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "eslint": "8.51.0",
    "react": "18.2.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "keywords": [
    "build",
    "icon",
    "svg",
    "incremental",
    "jit"
  ],
  "author": {
    "email": "vittorio.vittori@maggioli.it",
    "name": "Vittorio Vittori",
    "role": "Design System Architect",
    "url": "https://github.com/vitto"
  },
  "contributors": [
    {
      "name": "Andrea Pruccoli",
      "email": "andrea.pruccoli@maggioli.it",
