{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "outDir": "./lib",
    "skipLibCheck": true,
  },
  "include": [
    "src",
    ],
  "exclude": [
    "lib",
    "node_modules"
  ]
}
